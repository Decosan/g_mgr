<div class = 'container'>
  <% render 'layouts/flash_messages' %>
  <h1>Users Index</h1>
  <ul class="media-list">
    <% @users.each do |user| %>
      <li class="media">
        <h4 class="media-left" href="#">
          ICON:
        </h4>
        <div class="media-body">
          <h4 class="media-heading">Name:<%= user.name %></h4>
          Registerd: <%= user.created_at %>
        </div>
      </li>
    <% end %>  
  </ul>
</div>